<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccc.demoboot.mapper.WarehouseMapper">
    <select id="selectByGoodid" resultType="com.ccc.demoboot.domain.Warehouse">
        SELECT t.*
        FROM wms.warehouse t
        where goodid = #{goodid}
    </select>
</mapper>